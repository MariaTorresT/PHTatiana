<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-configuration PUBLIC "-//Hibernate/Hibernate Configuration DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-configuration-3.0.dtd">
<hibernate-configuration>
  <session-factory>
    <property name="hibernate.dialect">org.hibernate.dialect.H2Dialect</property>
    <property name="hibernate.connection.driver_class">org.h2.Driver</property>
    <property name="hibernate.connection.url">jdbc:h2:mem:test</property>    
    <property name="hibernate.hbm2ddl.auto">create</property>
    <property name="hibernate.show_sql">true</property>
    
    <mapping class="edu.eci.cosw.samples.model.Autorizacion"/>
    <mapping class="edu.eci.cosw.samples.model.Contrato"/>
    <mapping class="edu.eci.cosw.samples.model.Cotizacion"/>
    <mapping class="edu.eci.cosw.samples.model.Despacho"/>
    <mapping class="edu.eci.cosw.samples.model.DetalleInventario"/>
    <mapping class="edu.eci.cosw.samples.model.DetalleOrdenCompra"/>
    <mapping class="edu.eci.cosw.samples.model.DetallePedido"/>
    <mapping class="edu.eci.cosw.samples.model.Devolucion"/>
    <mapping class="edu.eci.cosw.samples.model.Epsafilida"/>
    <mapping class="edu.eci.cosw.samples.model.Factura"/>
    <mapping class="edu.eci.cosw.samples.model.Inventario"/>
    <mapping class="edu.eci.cosw.samples.model.ItemDeFactura"/>
    <mapping class="edu.eci.cosw.samples.model.Medicamento"/>
    <mapping class="edu.eci.cosw.samples.model.MedicamentoPorProveedor"/>
    <mapping class="edu.eci.cosw.samples.model.Mensajero"/>
    <mapping class="edu.eci.cosw.samples.model.OrdenCompra"/>
    <mapping class="edu.eci.cosw.samples.model.Paciente"/>
    <mapping class="edu.eci.cosw.samples.model.Pedido"/>
    <mapping class="edu.eci.cosw.samples.model.Proveedor"/>
    

    </session-factory>  
</hibernate-configuration>


 